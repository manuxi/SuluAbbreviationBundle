{
    "name": "manuxi/sulu-abbreviations-bundle",
    "description": "Say hello to Sulu abbreviations!",
    "type": "symfony-bundle",
    "keywords": [
        "sulu",
        "suluCms",
        "abbreviations"
    ],
    "homepage": "https://github.com/manuxi/SuluAbbreviationsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuxi",
            "email": "manu@bytefresser.de"
        }
    ],
    "require": {
        "dantleech/phpcr-migrations-bundle": "^1.2",
        "php": "^8.1",
        "sulu/sulu": "^2.5",
        "symfony/config": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/intl": "^6.2",
        "symfony/translation": "^6.2",
        "symfony/security-core": "^6.3",
        "sensio/framework-extra-bundle": "^6.2"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^6.2",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/console": "^6.2",
        "phpspec/prophecy": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\SuluAbbreviationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\SuluAbbreviationsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        }
    }
}
